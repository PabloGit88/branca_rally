<table class="tableDownloadExcel" style="display:none" >
	<thead>
		<tr>
			<th>ID</th>
			<th>Nombre y Apellido</th>
			<th>DNI</th>
			<th>Email</th>
			<th>Telefono</th>
			<th>Carrera</th>
			<th>Equipo</th>
			<th>Compartido</th>
		</tr>
	</thead>
	<tbody>
		{% for item in paginator.currentPageResults %}
		<tr>
			<td>{{ item.id }}</td>
			<td>{{ item.user.fullname }}</td>
			<td>{{ item.user.dni }}</td>
			<td>{{ item.user.email }}</td>
			<td>{{ item.user.phone }}</td>
			<td>{{ item.race.name }}</td>
			<td>{{ item.team }}</td>
			{% if item.isFacebookShared %}
				<td>Si</td>
			{% else %}
				<td>No</td>
			{% endif %}
		</tr>
        {% endfor %}
	</tbody>
</table>
<button class="btn btn-success pull-right download">Exportar Excel</button>
